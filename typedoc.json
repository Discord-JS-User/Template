{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": ["./src"],
	"out": "docs",
	"name": "Discord.js User Template",
	"includeVersion": true,
	"readme": "./README.md",
	"commentStyle": "all",
	"tsconfig": "./tsconfig.json",
	"replaceText": {
		"inCodeCommentText": true,
		"inCodeCommentTags": true,
		"inIncludedFiles": true,
		"replacements": [
			{
				"pattern": "regex pattern",
				"flags": "regex flags",
				"replace": "replace with"
			}
		]
	}
}
